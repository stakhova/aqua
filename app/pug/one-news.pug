include mixin/mixin
doctype html
html
    head
        include componets/head
    body
        .wrapper
            include componets/header
            main
                .container
                    .sale-banner
                        img(src="img/sale-banner.jpg")
                        .sale-banner-title
                            h1 Фурнитура для ванны
                            .sale-banner-desc
                                p ALWAYS
                                    span 25%
                                    | OFF OR MORE
                    .breadcrumbs
                        ul.breadcrumbs-list
                            li
                                a(href="index.html") Главная
                            li
                                a(href="news.html") Новости
                            li
                                a(href="one-news.html") Название новости
                    .one-news-wrap
                        .main-title
                            p Название новости
                        .one-news
                            .one-news-slider
                                .one-news-item.clearfix
                                    .news-img
                                        img(src="img/one-news.png")
                                        .news-date-block
